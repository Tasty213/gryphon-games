    "repository": {
        "type": "git",
        "url": "https://github.com/tasty213/gryphon-games"
    },
    "devDependencies": {
        "cross-env": "^7.0.3",
        "css-loader": "^6.7.1",
        "eslint": "^8.15.0",
        "eslint-config-google": "^0.14.0",
        "jest": "^28.1.0",
        "jest-environment-jsdom": "^28.1.0",
        "style-loader": "^3.3.1",
        "webpack": "^5.72.1",
        "webpack-cli": "^4.9.2"
